{
  "name": "Daciolo Chrome Extension",
  "version": "1.1",
  "description": "Gloria a deuxxx",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["build/vendor.bundle.js", "build/app.bundle.js"]
    }
  ],
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}
